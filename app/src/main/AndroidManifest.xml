<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zemoso.zinteract.ZinteractSampleApp" >

    <uses-permission android:name="android.permission.INTERNET" />
    <!--<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>-->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.GET_ACCOUNTS" /><!--necessary only if if the device is running a version lower than Android 4.0.4 -->
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.zemoso.zinteract.ZinteractSampleApp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zemoso.zinteract.ZinteractSampleApp.Activity2"
            android:label="@string/title_activity_activity2" >
        </activity>
        <activity
            android:name="com.zemoso.zinteract.ZinteractSampleApp.Activity3"
            android:label="@string/title_activity_activity3" >
        </activity>
        <activity
            android:name="com.zemoso.zinteract.ZinteractSampleApp.Activity4"
            android:label="@string/title_activity_activity4" >
        </activity>
        <activity
            android:name="com.zemoso.zinteract.ZinteractSampleApp.Activity5"
            android:label="@string/title_activity_activity5" >
        </activity>
        <activity
            android:name="com.zemoso.zinteract.sdk.InAppMessage"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
    </application>

</manifest>
