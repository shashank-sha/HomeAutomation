<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zemosolabs.zinteract" >

    <application
        android:allowBackup="true"
        android:label="@string/app_name" >
        <service android:name=".sdk.GcmIntentService" />
        <service
            android:name=".sdk.GeofenceTransitionsIntentService"
            android:exported="false" >
        </service>

        <receiver
            android:name=".sdk.MyGeofenceRelatedReceiver"
            android:enabled="true"
            android:exported="false" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.location.MODE_CHANGED" />
                <action android:name="android.location.PROVIDERS_CHANGED" />
            </intent-filter>
        </receiver>

        <service
            android:name=".sdk.CampaignHandlingService"
            android:enabled="true"
            android:permission=""
            android:exported="false" >
        </service>
    </application>

</manifest>
